<div class="container-fluid">
    <div class="col-12">
        <section class="content-header">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb float-right small d-none d-sm-inline-flex">
                    <li class="breadcrumb-item"><a href="@Url.Action("Index", "Home")"><i class="fa fa-home">&nbsp;</i>Inicio</a></li>
                    <li class="breadcrumb-item"><span><i class="fa fa-search">&nbsp;</i>Consultas </span></li>
                    <li class="breadcrumb-item active"><span><i class="fa fa-database">&nbsp;</i>@ViewBag.Title </span></li>
                </ol>
            </nav>
            <h1 class="">@ViewBag.Title <small></small></h1>
        </section>
    </div>
    <br />
    <div class="col-12 mt-4">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body bg-light-gray">
                        <form action="@Url.Action("GenerarArchivosBancos", "Pagos")" method="GET" target="_blank">

                            <div class="form-row">
                                <div class="form-group col-lg-1 col-md-2 col-sm-4 col-6">
                                    <label for="cmbAnio">Año</label>
                                    @Html.DropDownList("cmbAnio", new SelectList(ViewBag.Anios, "Value", "TextDisplay", ViewBag.CurrentYear), new { @class = "form-control form-control-sm" })
                                </div>
                                <div class="form-group col-lg-1 col-md-2 col-sm-4 col-6">
                                    <label for="cmbPeriodoGrupal">Periodo</label>
                                    @Html.DropDownList("cmbPeriodo", new SelectList(ViewBag.Periodos, "Value", "TextDisplay", ViewBag.DefaultPeriodo), null, new { @class = "form-control form-control-sm" })
                                </div>

                                <div class="form-group col-lg-1 col-md-2 col-sm-4 col-6">
                                    <label for="cmbTipoEstudio">Tipo de estudio</label>
                                    @Html.DropDownList("cmbTipoEstudio", new SelectList(ViewBag.TipoEstudios, "Value", "TextDisplay", ViewBag.DefaultTipoEstudio), null, new { @class = "form-control form-control-sm" })
                                </div>

                                <div class="form-group col-lg-3 col-md-5 col-sm-12 col-12">
                                    <label for="cmbFacultad">Facultades/Especialidades</label>
                                    @Html.DropDownList("cmbFacultad", new SelectList(ViewBag.Facultades, "Value", "TextDisplay", ViewBag.DefaultFacultad), "TODOS", new { @class = "form-control form-control-sm" })
                                </div>

                                <div class="form-group col-lg-2 col-sm-3 col-12">
                                    <button type="submit" class="btn btn-block btn-outline-primary mt-4">
                                        <i class="fa fa-fw fa-engine">&nbsp;</i> Generar obligaciones
                                    </button>
                                </div>
                            </div>

