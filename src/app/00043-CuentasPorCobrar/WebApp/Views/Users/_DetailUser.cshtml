
@model WebApp.ViewModels.UserRegisterViewModel

<div class="modal-header bg-dark-gray" style="border-radius: .2rem .2rem 0px 0px">
    <h5 class="modal-title text-center"><i class="fa fa-user">&nbsp;</i>&nbsp;@ViewBag.Title</h5>
    <button id="btn-close-header" type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
</div>
<div class="modal-body">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-5">
                @Html.LabelFor(model => model.UserName, new { @class = "control-label" })
            </div>
            <div class="col-md-7">
                @Html.DisplayFor(model => model.UserName)
            </div>
        </div>

        <div class="row">
            <div class="col-md-5">
                @Html.LabelFor(model => model.RoleName, new { @class = "control-label" })
            </div>
            <div class="col-md-7">
                @Html.DisplayFor(model => model.RoleName)
            </div>
        </div>

        <div class="row">
            <div class="col-md-5">
                @Html.LabelFor(model => model.PersonName, new { @class = "control-label" })
            </div>
            <div class="col-md-7">
                @Html.DisplayFor(model => model.PersonName)
            </div>
        </div>

        @*<div class="row">
            <div class="col-md-4">
                @Html.LabelFor(model => model.numDoc, new { @class = "control-label" })
            </div>
            <div class="col-md-8">
                @Html.DisplayFor(model => model.numDoc)
            </div>
        </div>*@

        <div class="row">
            <div class="col-md-5">
                @Html.LabelFor(model => model.Email, new { @class = "control-label" })
            </div>
            <div class="col-md-7">
                @Html.DisplayFor(model => model.Email)
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                @Html.LabelFor(model => model.Dependencia, new { @class = "control-label" })
            </div>
            <div class="col-md-8 col-md-offset-5">
                <p>@Model.Dependencia</p>
</div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
</div>
